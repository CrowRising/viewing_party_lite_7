<%= link_to 'Home', root_path %>
<h1>Viewing Party</h1>

 <% if session[:user_id] %>
    <%= link_to "Log Out", logout_path %>
    <h2>Existing Users:</h2>
    <ul>
      <% @users.each do |user| %>
        <li><%= user.name %></li>
      <% end %>
    </ul>
  <% else %>
    <%= link_to 'Create a New User', new_user_path, class: 'button' %>
    <%= link_to "Log In", login_path %>
  <% end %>  
  